
<%= form_with(model: @room, remote: true, data: { controller: "reset-form", action: "turbo:submit-end->reset-form#reset" }) do |f| %>
  <div class="form-box">
    <%= f.text_field :name, class: 'form-control', autocomplete: 'off', class:"input-box"  %>
    <%= f.submit data: {disable_with: false}, class:"select-button" %>
  </div>
<% end %>
